<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Rooms - Travlr Getaways Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header }}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Rooms</h1>
							<ul id="rooms">
								{{#each room_info}}
								<li>
									<a href="{{this.link_one}}"><img src="images/{{this.image}}" alt="{{this.image_alt}}"></a>
									<h2><a href="{{this.link_two}}">{{this.name}}</a></h2>
									{{{this.description}}}
									<span class="rate">Rate: {{this.rate}} / Day</span>
								</li>
								{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer }}
	</div>
</body>
</html>